{
  "name": "openlayers-react",
  "version": "0.0.1",
  "description": "OpenLayer React Components",
  "private": true,
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack-dev-server --port 9001 --hot --config examples/config/webpack.config.serve.js --open",
    "check-types": "tsc",
    "clean": "rimraf dist",
    "build": "npm-run-all --serial clean build:lib",
    "build:lib": "cross-env NODE_ENV=production rollup --config ./config/rollup.config.js",
    "prebuild:demo": "rimraf examples/dist",
    "build:demo": "cross-env NODE_ENV=production webpack --config examples/config/webpack.config.build.js",
    "lint": "eslint --cache \"+(src|examples)/**/*.ts?(x)\""
  },
  "keywords": [
    "openlayer",
    "typescript",
    "react",
    "map"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@hot-loader/react-dom": "16.8.x",
    "@material-ui/core": "^4.3.2",
    "@types/google-maps": "^3.2.0",
    "@types/googlemaps": "^3.26.4",
    "@types/node": "^10.12.18",
    "@types/ol": "^5.3.3",
    "@types/react": "16.8.x",
    "@types/react-dom": "16.8.x",
    "@types/react-router": "4.x.x",
    "@types/react-router-dom": "4.x.x",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "5.x.x",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "ol": "^5.3.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "16.8.x",
    "react-dom": "16.8.x",
    "react-hot-loader": "^4.12.11",
    "react-router": "4.3.x",
    "react-router-dom": "4.3.x",
    "react-syntax-highlighter": "^11.0.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "typescript": "3.5.3",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  },
  "peerDependencies": {
    "ol": "5.x.x",
    "react": "^16.7.0"
  },
  "dependencies": {}
}
